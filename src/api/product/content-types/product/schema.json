{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "discreption": {
      "type": "richtext"
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "instantDelivery": {
      "type": "boolean"
    },
    "files": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "whatsIncluded": {
      "type": "richtext"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "أجهزة الضغط",
        "الأم والطفل",
        "مستلزمات الجراحية",
        "ضمادات",
        "علاج طبيعي",
        "كبر السن",
        "كمادات",
        "مرضى السكر",
        "مستلزمات التجميل",
        "مستهلكات"
      ]
    },
    "carts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cart.cart",
      "mappedBy": "products"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "mappedBy": "products"
    }
  }
}
